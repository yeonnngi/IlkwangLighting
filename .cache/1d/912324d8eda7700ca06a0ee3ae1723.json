gin-top: 14px;\r\n}\r\n\r\n.popBox button:hover{\r\n  background-color: #FD8234;\r\n}\r\n\r\n.popBox button span{\r\n  color: #fff;\r\n  font-size: 13px;\r\n  line-height: 37px;\r\n  font-weight: bold;\r\n}\r\n\r\n\r\n/* --------------- find_pw ---------------  */\r\n.main {\r\n  width: 100%;\r\n  height: auto;\r\n  padding: 50px 0 0;\r\n}\r\n\r\n.wrap {\r\n  width: 400px;\r\n  height: auto;\r\n  margin: 105px auto 150px;\r\n}\r\n\r\n.find_pw{\r\n  font-size: 15px;\r\n  margin: 140px auto 20px;\r\n}\r\n\r\n.find_pw p{\r\n  font-size: 14px;\r\n  padding: 8px 0 5px;\r\n}\r\n\r\n.findPw li label{\r\n  font-size: 14px;\r\n}\r\n\r\n.userName,\r\n.userId,\r\n.userTel{\r\n  width: 400px; height: 45px;\r\n  padding: 10px 15px; box-sizing: border-box;\r\n  margin: 5px auto 15px;\r\n  font-size: 14px;\r\n}\r\n\r\ninput::placeholder{\r\n  font-size: 11px;\r\n}\r\n\r\n.checkBtn{\r\n  width: 400px; height: 50px;\r\n  background-color: #EFE8DA;\r\n  border: none;\r\n  text-align: center;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  line-height: 50px;\r\n  letter-spacing: 1px;\r\n  margin: 10px auto 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.checkBtn:hover{\r\n  background-color: #FD8234;\r\n  color: #fff;\r\n}\r\n\r\n.loginBtn{\r\n  width: 400px; height: 50px;\r\n  background-color: #261C13;\r\n  border: none;\r\n  text-align: center;\r\n  line-height: 50px;\r\n  letter-spacing: 1px;\r\n  margin: 5px auto 20px;\r\n  cursor: pointer;\r\n}\r\n\r\n.loginBtn a{\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  color: #fff;\r\n}\r\n\r\n.loginBtn:hover{\r\n  background-color: #FD8234;\r\n}\r\n"},"lineCount":183}},"error":null,"hash":"6b2236ca2f2a04f44ccaf92e5213ec0d","cacheData":{"env":{}}}