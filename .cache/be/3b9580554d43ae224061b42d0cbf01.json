ht: auto;\r\n  margin: 0 auto;\r\n}\r\n\r\n.terms .termsAll {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.terms .termsAll .total_terms {\r\n  width: 17px;\r\n  height: 17px;\r\n  margin-top: 5px;\r\n}\r\n\r\n.terms .termsAll label span {\r\n  padding: 0 5px;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n}\r\n\r\nhr {\r\n  margin: 20px 0 10px;\r\n}\r\n\r\n.terms li h4 {\r\n  padding: 10px 0;\r\n}\r\n\r\n.terms li h4 span {\r\n  color: #FD8234;\r\n}\r\n\r\n.terms li .terms_box {\r\n  width: 500px;\r\n  height: 300px;\r\n  border: 1px solid #261C13;\r\n  overflow: scroll;\r\n  padding: 20px 30px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.terms .choice .terms_box {\r\n  width: 500px;\r\n  height: 200px;\r\n}\r\n\r\n.terms li .terms_box p {\r\n  font-size: 13px;\r\n}\r\n\r\n.terms .terms_chk {\r\n  display: flex;\r\n  align-items: center;\r\n  margin: 5px auto 30px;\r\n}\r\n\r\n.terms .terms_chk .terms_el {\r\n  width: 16px;\r\n  height: 16px;\r\n  margin: 4px 6px 0 10px;\r\n}\r\n\r\n.terms li .terms_chk span {\r\n  font-size: 14px;\r\n  padding: 5px 0;\r\n}\r\n\r\n.terms li .terms_chk label span {\r\n  font-size: 14px;\r\n  margin-bottom: 5px;\r\n  padding: 0;\r\n  cursor: pointer;\r\n}\r\n\r\n.next_btn {\r\n  width: 500px;\r\n  height: 60px;\r\n  background-color: #261C13;\r\n  text-align: center;\r\n  margin: 50px auto 150px;\r\n  color: #EFE8DA;\r\n  cursor: pointer;\r\n}\r\n\r\n.next_btn:hover {\r\n  background-color: #FD8234;\r\n}\r\n\r\n.next_btn span {\r\n  color: #fff;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  line-height: 60px;\r\n  letter-spacing: 2px;\r\n}\r\n\r\n\r\n/* ----------------------- top_btn ----------------------- */\r\n.top_btn {\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 50%;\r\n  background-color: #191919;\r\n  position: fixed;\r\n  bottom: 50px;\r\n  right: 50px;\r\n  text-align: center;\r\n  z-index: 999;\r\n  cursor: pointer;\r\n}\r\n\r\n.top_btn i {\r\n  text-align: center;\r\n  line-height: 40px;\r\n  color: #fff;\r\n}"},"lineCount":null}},"error":null,"hash":"103c061e1fde8fcdda25d55b0910b0ad","cacheData":{"env":{}}}